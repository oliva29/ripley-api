{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "es6": true,
        "mocha": true
    },
    "extends": [
        "eslint:recommended",
        "prettier",
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "warn",
        "camelcase": "off",
        "comma-dangle": "error"
    }
}
